<FactDictionaryModule>
  <Facts>
    <Fact path="/testFactWithLimits">
      <Name>Test Fact</Name>
      <Description>test fact description</Description>
      <Writable>
                <!-- first limit -->
        <Limit type="MinLength">
          <Int>12</Int>
        </Limit>

                <!-- writable type -->
        <String />

                <!-- second limit -->
        <Limit type="MaxLength" level="warn">
          <Int>50</Int>
        </Limit>
      </Writable>
    </Fact>
  </Facts>
</FactDictionaryModule>
