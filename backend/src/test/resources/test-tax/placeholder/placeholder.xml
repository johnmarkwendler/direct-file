<FactDictionaryModule>
  <Facts>
    <Fact path="/someString">
      <Name>a string</Name>
      <Description>a string</Description>
      <Writable>
        <String />
      </Writable>
    </Fact>

    <Fact path="/factWithPlaceholder">
      <Name>Fact with placeholder</Name>
      <Description>Fact with placeholder</Description>
      <Writable>
        <String />
      </Writable>
      <Placeholder>
        <Dependency path="/someString" />
      </Placeholder>
    </Fact>
  </Facts>
</FactDictionaryModule>
